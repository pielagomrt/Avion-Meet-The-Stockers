STOCKS
<table>
  <thead>
    <tr>
      <th scope="col"></th>
      <th scope="col">BUYER STOCK ID</th>
      <th scope="col">Stock ID</th>
      <th scope="col">Ticker</th>
      <th scope="col">Company</th>
      <th scope="col">Price</th>
      <th scope="col">Broker ID</th>
      <th scope="col">Broker</th>
      <th scope="col">Buyer</th>
      <th></th>
    </tr>
  </thead>
  <% @buyer_stocks.each do |buyer_stock| %>
    <tbody>
      <tr>
        <td scope="row"></td>
        <td><%= buyer_stock.id %></td>
        <td><%= buyer_stock.broker_stock.stock_id %></td>
        <td><%= buyer_stock.broker_stock.stock.ticker %></td>
        <td><%= buyer_stock.broker_stock.stock.company %></td>
        <td><%= buyer_stock.broker_stock.stock.price %></td>
        <td><%= buyer_stock.broker_stock.broker_id %></td>
        <td><%= buyer_stock.broker_stock.broker.first_name %> <%= buyer_stock.broker_stock.broker.last_name %></td>
        <td><%= buyer_stock.buyer.first_name %> <%= buyer_stock.buyer.last_name %></td>
        <td></td>
        </tr>
    </tbody>
<% end %>
</table>



           
            


