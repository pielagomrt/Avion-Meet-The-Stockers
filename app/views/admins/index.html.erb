BROKERS
<table>
  <thead>
    <tr>
      <th scope="col"></th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th></th>
    </tr>
  </thead>
  <% @brokers.each do |broker| %>
    <tbody>
      <tr>
        <td scope="row"></td>
        <td><%= broker.first_name %></td>
        <td><%= broker.last_name %></td>
        <td><%= broker.email %></td>
      </tr>
    </tbody>
  <% end %>
</table>

